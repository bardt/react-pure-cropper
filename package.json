{
  "name": "react-pure-cropper",
  "version": "0.0.1",
  "description": "A set of React.js stateless components for image cropping and preview",
  "main": "lib/index.js",
  "engines": {
    "node": "4.2.1",
    "npm": "3.3.12"
  },
  "scripts": {
    "build": "babel src -r lib",
    "demo": "tooling watch --entry ./example/index.js",
    "demo:build": "tooling build --entry ./example/index.js",
    "build:redux": "tooling build --entry ./example/redux-example/index.js",
    "demo:redux": "tooling watch --entry ./example/redux-example/index.js",
    "test": "nyc --reporter=lcov ava"
  },
  "peerDependencies": {
    "react": "^0.14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bardt/react-pure-cropper.git"
  },
  "keywords": [
    "react",
    "cropper",
    "image",
    "preview",
    "stateless",
    "redux",
    "pure"
  ],
  "author": "Roman Salnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bardt/react-pure-cropper/issues"
  },
  "homepage": "https://github.com/bardt/react-pure-cropper#readme",
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "deep-freeze": "0.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "nyc": "^5.5.0",
    "react-addons-perf": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "tooling": "^0.6.2",
    "react-redux": "4.4.1",
    "redux": "3.3.1"
  }
}
